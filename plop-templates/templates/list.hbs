import React from "react";
import { connect } from "@sy/framework";
import { TablePF } from "@sy/platform/lib/components";

class List extends React.PureComponent {
    getColumns = () => {
        return [];
    };

    render() {
        const modelProps = {
            actionType: "",
            columns: this.getColumns(),
            isGetTableInstance: true,
            reload: false,
            rowKey: "",
            showPagination: true,
            tableId: "tableId"
        };
        return (
            <div className={`divComponent`}>
                <TablePF modelProps={ modelProps } />
            </div>
        );
    }
}

export default connect(({ current, tableConfig }) => {
    return {
        ...current,
        tableConfig
    };
})(List);
